{
  "id": 7,
  "locale": "ar",
  "english_name": "Arabic",
  "native_name": "العربية",
  "right_to_left": true,
  "flag_url": "https://d23w63adpjpix6.cloudfront.net/58/38/60/78/39b669a00d962e241.png",
  "contexts": {
    "date": {
      "rules": {
        "future": {
          "description": "{token} is in the past",
          "conditions": "(< @date (today))"
        },
        "past": {
          "description": "{token} is in the future",
          "conditions": "(> @date (today))"
        },
        "present": {
          "description": "{token} is in the present",
          "conditions": "(= @date (today))"
        }
      },
      "keys": [
        "past",
        "present",
        "future"
      ],
      "default_key": "present",
      "token_expression": "/.*(date|time)(\\d)*$/",
      "variables": [
        "@date"
      ],
      "token_mapping": [
        "unsupported",
        "unsupported",
        {
          "past": "{$0}",
          "present": "{$1}",
          "future": "{$2}"
        }
      ]
    },
    "number": {
      "rules": {
        "few": {
          "description": "{token} mod 100 in 3..10",
          "examples": "3-10, 103-110, 203-210...",
          "conditions": "(in '3..10' (mod @n 100))"
        },
        "many": {
          "description": "{token} mod 100 in 11..99",
          "examples": "11-99, 111-199, 211-299...",
          "conditions": "(in '11..99' (mod @n 100))"
        },
        "one": {
          "description": "{token} is 1",
          "examples": "1",
          "conditions": "(= @n 1)"
        },
        "other": {
          "examples": "100-102, 200-202, 300-302...; 0.2, 1.07, 2.94, 3.81, 11.68, 100.55..."
        },
        "two": {
          "description": "{token} is 2",
          "examples": "2",
          "conditions": "(= 2 @n)"
        },
        "zero": {
          "description": "{token} is 0",
          "examples": "0",
          "conditions": "(= 0 @n)"
        }
      },
      "keys": [
        "zero",
        "one",
        "two",
        "few",
        "many",
        "other"
      ],
      "default_key": "other",
      "token_expression": "/.*(count|num|minutes|seconds|hours|sum|total)(\\d)*$/",
      "variables": [
        "@n"
      ],
      "token_mapping": "unsupported"
    },
    "list": {
      "rules": {
        "one": {
          "description": "{token} contains 1 element",
          "conditions": "(= 1 @count)"
        },
        "other": {
          "description": "{token} contains at least 2 elements"
        }
      },
      "keys": [
        "one",
        "other"
      ],
      "default_key": "other",
      "token_expression": "/.*(items|list)(\\d)*$/",
      "variables": [
        "@count"
      ],
      "token_mapping": [
        "unsupported",
        {
          "one": "{$0}",
          "other": "{$1}"
        }
      ]
    },
    "genders": {
      "rules": {
        "female": {
          "description": "{token} contains 1 female",
          "conditions": "(&& (= 1 (count @genders)) (all @genders 'female'))"
        },
        "male": {
          "description": "{token} contains 1 male",
          "conditions": "(&& (= 1 (count @genders)) (all @genders 'male'))"
        },
        "other": {
          "description": "{token} contains at least 2 people"
        },
        "unknown": {
          "description": "{token} contains 1 person with unknown gender",
          "conditions": "(&& (= 1 (count @genders)) (all @genders 'unknown'))"
        }
      },
      "keys": [
        "male",
        "female",
        "unknown",
        "other"
      ],
      "default_key": "other",
      "token_expression": "/.*(users|profiles|actors|targets)(\\d)*$/",
      "variables": [
        "@genders"
      ],
      "token_mapping": [
        "unsupported",
        "unsupported",
        "unsupported",
        {
          "male": "{$0}",
          "female": "{$1}",
          "unknown": "{$2}",
          "other": "{$3}"
        }
      ]
    }
  },
  "cases": {
  }
}